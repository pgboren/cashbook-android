<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="CustomChipChoiceStyle" format="reference"/>

    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <!-- {@deprecated Use civ_circlebackground_color instead.} -->
        <attr name="civ_fill_color" format="color" />
        <attr name="civ_circle_background_color" format="color" />
    </declare-styleable>

    <declare-styleable name="RtlMaterialSpinner">
        <attr name="ms_baseColor" format="integer"/>
        <attr name="ms_highlightColor" format="integer"/>
        <attr name="ms_errorColor" format="integer"/>
        <attr name="ms_error" format="string"/>
        <attr name="ms_hint" format="string"/>
        <attr name="ms_hintColor" format="color"/>
        <attr name="ms_floatingLabelText" format="string"/>
        <attr name="ms_floatingLabelColor" format="color"/>
        <attr name="ms_nbErrorLines" format="integer"/>
        <attr name="ms_typeface" format="string"/>
        <attr name="ms_alignLabels" format="boolean"/>
        <attr name="ms_thickness" format="dimension"/>
        <attr name="ms_thickness_error" format="dimension"/>
        <attr name="ms_arrowSize" format="dimension"/>
        <attr name="ms_arrowColor" format="color"/>
        <attr name="ms_enableFloatingLabel" format="boolean"/>
        <attr name="ms_enableErrorLabel" format="boolean"/>
        <attr name="ms_isRtl" format="boolean"/>
        <attr name="ms_hintTextSize" format="float"/>
        <attr name="list_item_image_bg" format="color" />

    </declare-styleable>

    <declare-styleable name="PageIndicatorView">
        <attr name="piv_orientation">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>

        <attr name="piv_viewPager" format="reference" />
        <attr name="piv_select" format="integer" />

        <attr name="piv_count" format="integer" />
        <attr name="piv_dynamicCount" format="boolean" />

        <attr name="piv_radius" format="dimension" />
        <attr name="piv_padding" format="dimension" />
        <attr name="piv_strokeWidth" format="dimension" />
        <attr name="piv_scaleFactor" format="float" />
        <attr name="piv_autoVisibility" format="boolean" />

        <attr name="piv_unselectedColor" format="color" />
        <attr name="piv_selectedColor" format="color" />

        <attr name="piv_fadeOnIdle" format="boolean" />
        <attr name="piv_idleDuration" format="integer" />

        <attr name="piv_interactiveAnimation" format="boolean" />
        <attr name="piv_animationDuration" format="integer" />
        <attr name="piv_animationType">
            <enum name="none" value="0" />
            <enum name="color" value="1" />
            <enum name="scale" value="2" />
            <enum name="worm" value="3" />
            <enum name="slide" value="4" />
            <enum name="fill" value="5" />
            <enum name="thinWorm" value="6" />
            <enum name="drop" value="7" />
            <enum name="swap" value="8" />
            <enum name="scale_down" value="9" />
        </attr>
        <attr name="piv_rtl_mode">
            <enum name="on" value="0" />
            <enum name="off" value="1" />
            <enum name="auto" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="DocLookupTextInputVIew">
        <attr name="dltv_title" format="string" localization="suggested" />
        <attr name="dltv_title_visible" format="boolean"/>
        <attr name="dltv_blank_message" format="string" localization="suggested" />
        <attr format="reference" name="dltv_endIconDrawable"/>
        <attr name="dltv_doc_name" format="string"/>
        <attr format="boolean" name="dltv_errorEnabled"/>
        <attr name="dltv_helperText" format="string"/>

    </declare-styleable>

    <declare-styleable name="DocLookUpInputView">
        <attr name="title" format="string" localization="suggested" />
        <attr name="title_visible" format="boolean"/>
        <attr name="blank_message" format="string" localization="suggested" />
        <attr format="reference" name="endIconDrawable"/>
        <attr name="documentPhotoVisible">
            <!-- Visible on screen; the default value. -->
            <enum name="visible" value="0" />
            <!-- Not displayed, but taken into account during layout (space is left for it). -->
            <enum name="invisible" value="4" />
            <!-- Completely hidden, as if the view had not been added. -->
            <enum name="gone" value="8" />
        </attr>
        <attr name="documentName" format="string"/>
    </declare-styleable>

    <declare-styleable name="DocItemsView">
        <attr name="div_title" format="string" localization="suggested" />
        <attr name="div_allow_many_items" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="AgileStageSummaryView">
        <attr name="stage_name" format="string" localization="suggested" />
        <attr name="stage_id" format="string" localization="suggested" />
        <attr name="stage_icon" format="string" localization="suggested" />
        <attr format="reference" name="stage_summary_iconDrawable"/>
    </declare-styleable>

    <declare-styleable name="PriceInputView">
        <attr name="piv_title" format="string"/>
    </declare-styleable>

    <declare-styleable name="DatePickerView">
        <attr name="dpv_label" format="string"/>
        <attr name="dpv_text_color" format="color"/>
        <attr name="dpv_require" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="PaymentOptionView">
        <attr name="potv_label" format="string"/>
        <attr name="potv_text_color" format="color"/>
        <attr name="potv_require" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="CouponNumberInputView">
        <attr name="cnv_title" format="string"/>
        <attr name="cnv_blank_message" format="string"/>
    </declare-styleable>

    <declare-styleable name="StageChipGroup">
        <attr name="selectedStage" format="string"/>
    </declare-styleable>

</resources>