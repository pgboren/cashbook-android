<?xml version="1.0" encoding="utf-8"?><androidx.coordinatorlayout.widget.CoordinatorLayout    xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent">    <include layout="@layout/layout_toolbar" />    <ScrollView        android:id="@+id/scrollable_container"        android:layout_width="match_parent"        android:fillViewport="true"        android:isScrollContainer="true"        app:layout_behavior="@string/appbar_scrolling_view_behavior"        android:layout_height="match_parent">        <LinearLayout            android:id="@+id/content_container"            android:paddingLeft="10dp"            android:paddingRight="10dp"            android:orientation="vertical"            android:layout_width="match_parent"            android:layout_height="match_parent">            <com.soleap.cashbook.common.widget.lookup.DocLookupTextInputVIew                app:dltv_title="@string/item"                app:dltv_doc_name="item"                app:dltv_helperText="@string/require"                android:layout_width="match_parent"                app:dltv_endIconDrawable="@drawable/ic_arrow_right"                android:layout_height="wrap_content"/>        </LinearLayout>    </ScrollView></androidx.coordinatorlayout.widget.CoordinatorLayout>